<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>

	<display-name>Simple Maven</display-name>
	<servlet>
		<servlet-name>SimpleServlet</servlet-name>
		<servlet-class>vn.myclass.servlet.SimpleServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>XuLyLogin</servlet-name>
		<display-name>XuLyLogin</display-name>
		<description></description>
		<servlet-class>vn.myclass.servlet.XuLyLogin</servlet-class>
	</servlet>

	<servlet>
		<servlet-name>XuLyLoginTrenServer</servlet-name>
		<display-name>XuLyLoginTrenServer</display-name>
		<description></description>
		<servlet-class>vn.myclass.servlet.XuLyLoginTrenServer</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>SimpleServlet</servlet-name>
		<url-pattern>/SimpleServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>XuLyLogin</servlet-name>
		<url-pattern>/XuLyLogin</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>XuLyLoginTrenServer</servlet-name>
		<url-pattern>/XuLyLoginTrenServer</url-pattern>
	</servlet-mapping>


</web-app>
